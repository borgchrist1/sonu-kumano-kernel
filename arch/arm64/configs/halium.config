CONFIG_DEVTMPFS=y
CONFIG_FHANDLE=y
CONFIG_SYSVIPC=y
CONFIG_IPC_NS=y
CONFIG_NET_NS=y
CONFIG_PID_NS=y
CONFIG_USER_NS=y
CONFIG_UTS_NS=y
CONFIG_VT=y
# CONFIG_BACKLIGHT_QCOM_SPMI_WLED is not set
CONFIG_DRM_SDE_SPECIFIC_PANEL=y
CONFIG_HALL_SENSOR_NOT_USE_SWITCH=y
CONFIG_DEFAULT_SECURITY="apparmor"
CONFIG_SW_SYNC_USER=y
CONFIG_NET_CLS_CGROUP=y
CONFIG_CGROUP_NET_CLASSID=y
CONFIG_VETH=y
CONFIG_NETFILTER_XT_TARGET_CHECKSUM=y
CONFIG_ANDROID_BINDER_DEVICES="binder,hwbinder,vndbinder,anbox-binder,anbox-hwbinder,anbox-vndbinder"
# Ensure overlay gets built
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# dependency to DRM SDE SPECIFIC PANEL
CONFIG_SOMC_CHARGER_EXTENSION=y

# pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_RAM=y
CONFIG_PSTORE_RAM_ANNOTATION_APPEND=y

# Touchscreen
CONFIG_INPUT_TOUCHSCREEN=y
CONFIG_TOUCHSCREEN_SEC_TS=y

# Fingerprint
CONFIG_INPUT_FP_EGIS=y
CONFIG_ET580_USE_TIMER_SETUP=y

# NFC
CONFIG_NFC_PN553_DEVICES=y

# Sony Camera
CONFIG_SONY_CAMERA=y
CONFIG_CAMERA_SYNC=y
CONFIG_FLASH_LED=y

# WLAN
CONFIG_QCA_CLD_WLAN=y

# AMS TCS3490 Color driver
CONFIG_INPUT_TCS3490=y

# Hall sensor
CONFIG_INPUT_BU520X1NVX=y

# Enable both bahamut and griffin. This essentially
# it's the upstream diffconfig content
CONFIG_DRM_SDE_SPECIFIC_PANEL=y
CONFIG_HALL_SENSOR_NOT_USE_SWITCH=y
CONFIG_MACH_SONY_GRIFFIN=y


